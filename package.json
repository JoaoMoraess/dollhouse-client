{
  "name": "dollhouse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tailwind-storybook": "postcss ./src/application/styles/globals.css -o ./public/storybook/tailwind.storybook.css",
    "tailwind-storybook-build": "NODE_ENV=production postcss ./src/application/styles/globals.css -o ./storybook-static/storybook/tailwind.storybook.css",
    "storybook": "yarn tailwind-storybook && start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook && yarn tailwind-storybook-build",
    "build-stats": "cross-env ANALYZE=true yarn build",
    "generate": "plop --plopfile generators/plopfile.js",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "test:base": "jest ",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --silent --maxWorkers=25%",
    "test:staged": "yarn test -- --findRelatedTests ./src",
    "test:ci": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "^0.4.1",
    "styled-jsx-plugin-postcss": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@next/bundle-analyzer": "^11.1.2",
    "@next/plugin-storybook": "^11.1.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "17.0.24",
    "@types/react-slick": "^0.23.5",
    "@types/recoil": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "autoprefixer": "^10.3.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^8.5.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jest": "^27.2.2",
    "jest-localstorage-mock": "^2.4.18",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "postcss": "^8.3.7",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.4.1",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.2.15",
    "ts-jest": "^27.0.5",
    "typescript": "4.4.3"
  }
}
